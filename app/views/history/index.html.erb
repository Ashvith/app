
<table>
  <thead>
    <tr>
      <th>Serial no</th>
      <th>User ID</th>
      <th>Created at</th>
      <th>Last seen</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id%></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
        <td><%= user.last_sign_in_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
